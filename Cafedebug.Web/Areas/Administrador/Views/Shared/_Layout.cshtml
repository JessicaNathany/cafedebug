<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Cafédebug</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/Administrator/plugins/fontawesome-free/css/all.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <!-- overlayScrollbars -->
    <link rel="stylesheet" href="~/Administrator/css/adminlte.min.css">
    <link rel="stylesheet" href="~/css/body.css">
    <link rel="stylesheet" href="~/css/body.min.css">
    <link rel="stylesheet" href="~/Administrator/css/body.min.css">

    <!-- Google Font: Source Sans Pro -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">

    <link rel="stylesheet" href="~/Administrator/plugins/datatables-bs4/css/dataTables.bootstrap4.min.css">
    <link rel="stylesheet" href="~/Administrator/plugins/datatables-responsive/css/responsive.bootstrap4.min.css">
</head>

<body class="hold-transition sidebar-mini">
    <div class="wrapper">

        <partial name="_Navbar" />

        <partial name="_SidebarMenu" />

        <div class="content-wrapper">
            <section class="content-header">
                <div class="container-fluid">
                    <div class="row mb-2">
                        <div class="col-sm-6">
                            <h1>Café Debbug</h1>
                        </div>
                        <div class="col-sm-6">
                            <ol class="breadcrumb float-sm-right">
                                <li class="breadcrumb-item"><a class="link-breadcrumb" href="#">Home</a></li>
                                <li class="breadcrumb-item active">Café Debbug</li>
                            </ol>
                        </div>
                    </div>
                </div>
            </section>

            <section class="content">
                <div class="card-body">
                    @RenderBody()
                </div>

            </section>
        </div>

        <partial name="_Footer" />

        <aside class="control-sidebar control-sidebar-dark">
        </aside>
    </div>
    <script src="~/Administrator/plugins/jquery/jquery.min.js"></script>
    <script src="~/Administrator/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/Administrator/js/adminlte.min.js"></script>
    <script src="~/Administrator/js/demo.js"></script>
    <script src="~/Administrator/plugins/datatables/jquery.dataTables.min.js"></script>
    <script src="~/Administrator/plugins/datatables-bs4/js/dataTables.bootstrap4.min.js"></script>
    <script src="~/Administrator/plugins/datatables-responsive/js/dataTables.responsive.min.js"></script>
    <script src="~/Administrator/plugins/datatables-responsive/js/responsive.bootstrap4.min.js"></script>
    <script src="~/Administrator/js/CustomTable.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
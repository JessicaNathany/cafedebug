
<div class="container-fluid">
    <div class="row">
        <nav class="navbar navbar-expand-lg">
            <!-- Logo -->
            <div class="col-5 col-sm-6 col-md-4 col-lg-2 col-xl-2">
                <div class="nav-logo">
                    <a class="Logo" href="@Url.Action("Index", "Home")">
                        <img src="https://cafedebug.s3.us-east-2.amazonaws.com/Imagens/logo-cafedebug.png" />
                    </a>
                </div>
            </div>
            <!-- Soical Media -->
            <div class="col-4 col-sm-3 col-md-3 d-lg-none">
                <div class="soical">
                    <a class="soical-media" href="https://www.youtube.com/channel/UCLI7ho2zV2apQ2WHPGxkCQw" target="_blank"><i class="fab fa-youtube"></i></a>
                    <a class="soical-media" href="https://twitter.com/cafedebug" target="_blank"><i class="fab fa-twitter-square"></i></a>
                </div>
            </div>
            <!-- Collapse button -->
            <button class="navbar-toggler Toggle-button" type="button" data-toggle="collapse" data-target="#navbarNavDropdown" aria-controls="navbarNavDropdown" aria-expanded="false" aria-label="Toggle navigation">
                <div class="animated-icon1">
                    <span></span>
                    <span></span>
                    <span></span>
                </div>
            </button>

            <!-- Menu -->
            <div class="col-sm-12 col-md-12 col-lg-9 col-xl-9">
                <div class="navbar-collapse collapse" id="navbarNavDropdown">
                    <ul class="navbar-nav">
                        <li class="nav-item active">
                            <a class="nav-link" href="#">@Html.ActionLink("Home", "Index", "Home") <span class="sr-only">(current)</span></a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#">@Html.ActionLink("Quem Somos", "Index", "About")</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#">@Html.ActionLink("Episódios", "Index", "Episode")</a>
                        </li>
                    </ul>
                </div>
            </div>

            <!-- Soical Media -->
            <div class="d-none d-lg-block d-xl-block col-lg-1 col-xl-1">
                <div class="soical">
                    <a class="soical-media" href="https://www.youtube.com/channel/UCLI7ho2zV2apQ2WHPGxkCQw" target="_blank"><i class="fab fa-youtube"></i></a>
                    <a class="soical-media" href="https://twitter.com/cafedebug" target="_blank"><i class="fab fa-twitter"></i></a>
                </div>
            </div>
        </nav>
    </div>
</div>

<script type="text/javascript">
    $(document).ready(function () {
        $('.Toggle-button').on('click', function () {
            $('.animated-icon1').toggleClass('open');
        });
    });
</script>






